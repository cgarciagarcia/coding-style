{
    "name": "worksome/coding-style",
    "description": "Worksomes coding style",
    "type": "composer-plugin",
    "require": {
        "PHP": "^8.0",
        "composer-plugin-api": "^2.0",
        "worksome/code-sniffer": "^0.7.1",
        "nunomaduro/larastan": "^1.0",
        "rector/rector": "^0.12.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan-mockery": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "pestphp/pest": "^1.21",
        "spatie/ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Worksome\\CodingStyle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worksome\\CodingStyle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Nybroe",
            "email": "oliver@worksome.com"
        }
    ],
    "extra": {
        "class": "Worksome\\CodingStyle\\Plugin"
    }
}
